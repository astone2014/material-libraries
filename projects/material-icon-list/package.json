{
  "name": "material-icon-list",
  "version": "0.0.1",
  "author": {
    "name": "Austen Stone"
  },
  "scripts": {
    "publish": "npm run package && npm run npm_publish",
    "package": "ng build material-icon-list --prod && npm run copy-readme && npm run npm_pack",
    "copy-readme": "cp ./README.md ./../../dist/material-icon-list",
    "npm_pack": "cd ./../../dist/material-icon-list && npm pack",
    "npm_publish": "cd ./../../dist/material-icon-list && npm publish"
  },
  "peerDependencies": {
    "@angular/common": "^11.2.1",
    "@angular/core": "^11.2.1",
    "@angular/material": "11.2.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
